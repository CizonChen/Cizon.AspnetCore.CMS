
@model  System.Collections.Generic.List<Cizon.CMS.MVC.Models.MenuModel>
    @for (int i = 0; i < Model.Count; i++)
{
        <li class="sidebar-nav-link">
            <a href="#" class="sidebar-nav-sub-title">
                <i class="@Model[i].IconUrl"></i>
                <span> @Model[i].Text</span>
                <span class="am-icon-chevron-down am-fr am-margin-right-sm sidebar-nav-sub-ico"></span>
            </a>
            <ul class="sidebar-nav sidebar-nav-sub">
                @foreach (var j in @Model[i].ChildMenuList)
                {
                    <li class="sidebar-nav-link"><a href="~/@j.Url" data-index="0"><span class="am-icon-angle-right sidebar-nav-link-logo"></span>@j.Text</a> </li>
            }
            </ul>
        </li>
}
